    "cheerio": "^1.1.0",
    "chromadb": "^3.0.9",
    "cohere-ai": "^7.17.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "@elevenlabs/elevenlabs-js": "^2.6.0",
    "express": "^5.1.0",
    "extract-json-from-string": "^1.0.1",
    "graphql": "^16.11.0",
    "joi": "^17.13.3",
    "joi-password-complexity": "^5.2.0",
    "js-tiktoken": "^1.0.20",
    "jsonrepair": "^3.13.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.30",
    "mime": "^4.0.7",
    "moment": "^2.30.1",
    "mssql": "^11.0.1",
    "multer": "^2.0.2",
    "mysql2": "^3.14.2",
    "ollama": "^0.5.16",
    "openai": "^5.10.1",
    "pg": "^8.16.3",
    "pinecone-client": "^2.0.0",
    "pluralize": "^8.0.0",
    "posthog-node": "^5.5.1",
    "prisma": "^6.12.0",
    "slugify": "^1.6.6",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.1",
    "truncate": "^3.0.0",
    "url-pattern": "^1.0.3",
    "uuid": "^11.1.0",
    "uuid-apikey": "^1.5.3",
    "weaviate-ts-client": "^2.2.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@inquirer/prompts": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-ft-flow": "^3.0.11",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "flow-bin": "^0.276.0",
    "flow-remove-types": "^2.276.0",
    "globals": "^16.3.0",
    "hermes-eslint": "^0.29.1",
    "node-html-markdown": "^1.3.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  },
  "overrides": {
    "inflight": "npm:lru-cache@^10.0.0",
    "npmlog": false,
    "are-we-there-yet": false,
    "gauge": false,
    "rimraf": "^6.0.0",
    "glob": "^11.0.0",
    "@grpc/grpc-js": "^1.12.4"
  }
}
